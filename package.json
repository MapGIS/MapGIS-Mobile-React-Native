{
  "name": "@mapgis/uniform-core-react-native",
  "version": "10.5.6",
  "description": "MapGIS Mobile for React Native，是MapGIS推出的一款基于React-Native的跨平台移动开发框架，用户可以基于它使用JavaScript开发出在Android和iOS平台下运行的原生移动GIS应用。",
  "main": "src/index.tsx",
  "files": [
    "src",
    "android",
    "ios",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "dev": "tsc -w",
    "build": "tsc",
    "build-docs": "npx typedoc --out doc/ --excludeNotExported --excludePrivate --excludeProtected --module commonjs --mode file src",
    "start": "npx react-native start",
    "android": "npx react-native  run-android",
    "release-android": "npx react-native run-android --variant=release",
    "reload": "adb reverse tcp:8081 tcp:8081 && adb shell input text rr",
    "ios": "npx react-native run-ios"
  },
  "keywords": [
    "react-native",
    "mapgis",
    "map"
  ],
  "repository": "https://github.com/MapGIS/Mapgis-Uniform-Core-React-Native",
  "author": "MapGIS (https://github.com/MapGIS/Mapgis-Uniform-Core-React-Native)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MapGIS/Mapgis-Uniform-Core-React-Native/issues"
  },
  "homepage": "https://github.com/MapGIS/Mapgis-Uniform-Core-React-Native#readme",
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "eslint": "^7.2.0",
    "jest": "^26.0.1",
    "react": "16.13.1",
    "react-native": "^0.67.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community"
    ],
    "rules": {}
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
